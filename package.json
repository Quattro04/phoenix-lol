{
  "name": "t-lol",
  "version": "1.0.0",
  "description": "Cheap and fast League of Legends accout leveling, accout selling, normal matches and more!",
  "author": "matija",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "build:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt build",
