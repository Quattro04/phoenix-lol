    "@nuxtjs/tailwindcss": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "push-dir": "^0.4.1"
  }
}
